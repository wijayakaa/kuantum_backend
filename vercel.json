{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["config/**", "routes/**"],
                "maxDuration": 10
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET,OPTIONS,PATCH,DELETE,POST,PUT",
                "Access-Control-Allow-Headers": "*"
            }
        }
    ]
}
